<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Version>1.0.0</Version>
        <Authors>Samuel Lucas</Authors>
        <Description>A .NET implementation of PBKDF2 with HMAC-SHA-2.</Description>
        <Copyright>Copyright (c) 2023 Samuel Lucas</Copyright>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/samuel-lucas6/PBKDF2.NET</PackageProjectUrl>
        <RepositoryUrl>https://github.com/samuel-lucas6/PBKDF2.NET</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>pbkdf2 hmac sha2 sha256 sha384 sha512</PackageTags>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    </PropertyGroup>

</Project>
